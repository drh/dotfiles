eval "$(/opt/homebrew/bin/brew shellenv)"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/drh/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/drh/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/drh/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/drh/google-cloud-sdk/completion.zsh.inc'; fi

export PATH="~/bin:~/go/bin:/usr/local/go/bin:$PATH";
export EDITOR="code --wait";
